<div id="<%= dom_id organization %>">
  <p>
    <strong>Name:</strong>
    <%= organization.name %>
  </p>

  <p>
    <strong>Url:</strong>
    <%= organization.url %>
  </p>

  <p>
    <strong>app type:</strong>
    <%= organization.app_type %>
  </p>

  <p>
    <strong>access user:</strong>
    <%= organization.access_user %>
  </p>

  <p>
    <strong>access token:</strong>
    <%= organization.access_token %>
  </p>

</div>

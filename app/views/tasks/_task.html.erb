<div id="<%= dom_id task %>">
  <p>
    <strong>Project:</strong>
    <%= task.project.name %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= task.name %>
  </p>

  <p>
    <strong>Url:</strong>
    <%= task.url %>
  </p>

  <p>
    <strong>Id task:</strong>
    <%= task.id_task %>
  </p>

  <p>
    <strong>Created:</strong>
    <%= task.created %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= task.status %>
  </p>

  <p>
    <strong>Priority:</strong>
    <%= task.priority %>
  </p>

  <p>
    <strong>Created by:</strong>
    <%= task.created_by %>
  </p>

  <p>
    <strong>Resolution:</strong>
    <%= task.resolution %>
  </p>

</div>
